{
  "name": "starter-kit-universally",
  "version": "1.4.0",
  "description": "A starter kit from Ueno. Based on react-universally",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=8 <10"
  },
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build": "npm run clean && cross-env NODE_ENV=production babel-node internal/scripts/build --optimize",
    "build:dev": "npm run clean && cross-env NODE_ENV=development babel-node internal/scripts/build",
    "build:static:prepare": "babel-node internal/scripts/static-site-generation/1-prepare.js",
    "build:static": "npm run build:static:prepare && babel-node internal/scripts/static-site-generation/2-renderHtml.js",
    "clean": "cross-env babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "dev": "cross-env DEPLOYMENT=development babel-node internal/development",
    "dev-remote": "cross-env DEPLOYMENT=development babel-node internal/scripts/ngrok",
    "preinstall": "node internal/scripts/preinstall",
    "heroku-postbuild": "npm run build",
    "start": "cross-env NODE_ENV=production node build/server",
    "start:static": "cross-env NODE_ENV=production node server/staticIndex.js",
    "lint": "npm run eslint && npm run stylelint",
    "lint-staged": "lint-staged",
    "eslint": "eslint client server shared config internal --report-unused-disable-directives",
    "stylelint": "stylelint \"shared/**/*.scss\" --syntax scss",
    "test": "npm run lint",
    "test-fails-on-herou": "jest --setupTestFrameworkScriptFile ./shared/setupTests.js",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ueno-llc/starter-kit-universally"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ueno-llc/starter-kit-universally/issues"
  },
  "homepage": "https://github.com/ueno-llc/starter-kit-universally",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "shared"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/internal/jest/styleMock.js",
      "\\.(je?pg|png|gif|ico|otf|ttf|eot|woff|woff2|svg)$": "<rootDir>/internal/jest/assetMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(.cache|build|internal|node_modules|flow-typed|public)/"
    ]
  },
  "pre-commit": "lint-staged",
  "lint-staged": {},
  "dependencies": {
    "app-root-dir": "1.0.2",
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "7.1.3",
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-css-modules-transform": "1.2.7",
    "babel-plugin-dynamic-import-node": "1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-universal-import": "1.2.7",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-template": "6.26.0",
    "basic-auth": "1.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chokidar": "1.7.0",
    "classnames": "2.2.5",
    "classnames-loader": "2.0.0",
    "colors": "1.1.2",
    "compression": "1.7.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "express": "4.15.4",
    "extract-css-chunks-webpack-plugin": "2.0.17",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "glob": "7.1.2",
    "happypack": "3.1.0",
    "helmet": "3.8.1",
    "hpp": "0.2.2",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "0.17.4",
    "json-stringify-safe": "5.0.1",
    "lodash": "4.17.4",
    "md5": "2.2.1",
    "mobx": "3.2.2",
    "mobx-react": "4.2.2",
    "mobx-react-devtools": "4.2.15",
    "node-notifier": "5.1.2",
    "node-sass": "4.5.3",
    "normalize.css": "7.0.0",
    "offline-plugin": "4.8.3",
    "postcss": "6.0.10",
    "postcss-csso": "2.0.0",
    "postcss-loader": "2.0.6",
    "pretty-error": "2.1.1",
    "prop-types": "15.5.10",
    "react": "16.0.0",
    "react-async-bootstrapper": "1.1.2",
    "react-dom": "16.0.0",
    "react-async-component": "1.0.2",
    "react-ga": "2.2.0",
    "react-helmet": "5.2.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-jobs": "1.0.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-universal-component": "2.5.5",
    "regenerator-runtime": "0.11.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "semver": "5.4.1",
    "serialize-javascript": "1.4.0",
    "sha256": "0.2.0",
    "source-map-support": "0.4.18",
    "style-loader": "0.18.2",
    "svg-to-jsx-loader": "2.0.1",
    "user-home": "2.0.0",
    "uuid": "3.1.0",
    "webpack": "3.6.0",
    "webpack-flush-chunks": "1.1.23",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.6.0",
    "write-file-webpack-plugin": "4.2.0"
  },
  "devDependencies": {
    "@ueno/eslint-config": "1.2.6",
    "@ueno/stylelint-config": "1.0.4",
    "babel-eslint": "7.2.3",
    "enzyme-to-json": "3.0.1",
    "babel-jest": "21.0.2",
    "eslint": "4.9.0",
    "fs-extra": "4.0.2",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "lint-staged": "4.2.2",
    "ngrok": "2.2.21",
    "onecolor": "3.0.4",
    "pre-commit": "1.2.2",
    "react-test-renderer": "16.0.0",
    "stylelint": "8.2.0",
    "superagent": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1"
  }
}
